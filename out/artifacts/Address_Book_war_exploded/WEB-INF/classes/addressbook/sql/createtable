CREATE TABLE person (
      id NUMBER NOT NULL,
      active VARCHAR2(5) NOT NULL,
      fname VARCHAR2(25),
      lname VARCHAR2(30),
      email VARCHAR2(35),
      streetaddress VARCHAR2(50),
      apartment VARCHAR2(15),
      cityaddress VARCHAR2(30),
      stateaddress VARCHAR2(25),
      zipaddress NUMBER(5),
      dateofbirth DATE,
      iamgebytes BLOB,
      imagetype VARCHAR2(20),
      constraint person_pk PRIMARY KEY (id)
);
CREATE SEQUENCE personsequence START WITH 1
INCREMENT BY 1
MINVALUE 1;